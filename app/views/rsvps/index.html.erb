<p id="notice"><%= notice %></p>

<h1>Listing Rsvps</h1>

<table>
  <thead>
    <tr>
      <th>Name1</th>
      <th>Name2</th>
      <th>Ceremony</th>
      <th>Reception</th>
      <th>Diner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rsvps.each do |rsvp| %>
      <tr>
        <td><%= rsvp.name1 %></td>
        <td><%= rsvp.name2 %></td>
        <td><%= rsvp.ceremony %></td>
        <td><%= rsvp.reception %></td>
        <td><%= rsvp.diner %></td>
        <td><%= link_to 'Show', rsvp %></td>
        <td><%= link_to 'Edit', edit_rsvp_path(rsvp) %></td>
        <td><%= link_to 'Destroy', rsvp, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rsvp', new_rsvp_path %>
